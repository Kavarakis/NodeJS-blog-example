{
  "name": "blog_example",
  "version": "1.0.0",
  "description": "Starter project",
  "main": "app.js",
  "scripts": {
    "lint:run": "eslint app.js ./",
    "migrate": "sequelize db:migrate",
    "drop": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "restartdb": "npm-run-all drop migrate seed start",
    "run:app": "sequelize db:migrate && babel-node app.js",
    "build": "webpack",
    "run:check": "babel-node app.js",
    "pretest": "npm-run-all drop migrate seed",
    "test": "set NODE_ENV=test&& mocha",
    "start": "nodemon --exec npm-run-all lint:run build run:app",
    "preserver": "webpack",
    "server": "webpack-dev-server"
  },
  "author": "Semir Salkic",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.7.2",
    "angular-jwt": "^0.1.10",
    "angular-route": "^1.7.2",
    "angular-storage": "0.0.15",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "mysql2": "^1.5.3",
    "npm": "^6.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "popper.js": "^1.14.3",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0"
