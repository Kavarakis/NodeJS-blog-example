{
  "name": "blog_example",
  "version": "1.0.0",
  "description": "Starter project",
  "main": "app.js",
  "scripts": {
    "lint:run": "eslint app.js ./",
    "migrate": "sequelize db:migrate",
    "drop": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "restartdb": "npm-run-all drop migrate seed start",
    "run:app": "sequelize db:migrate && babel-node app.js",
    "run:check": "babel-node app.js",
    "pretest": "npm-run-all drop migrate seed",
    "test": "set NODE_ENV=test&& mocha",
    "start": "nodemon --exec npm-run-all lint:run run:app"
  },
  "author": "Semir Salkic",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "helmet": "^3.10.0",
    "jsonwebtoken": "^8.1.1",
    "mysql2": "^1.5.1",
    "npm": "^5.7.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^4.32.6",
    "sequelize-cli": "^3.2.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "^4.1.2",
    "chalk": "^2.3.2",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "localtunnel": "^1.8.3",
    "mocha": "^5.1.1",
    "nodemon": "^1.14.12",
    "npm-run-all": "^4.1.2"
  }
}