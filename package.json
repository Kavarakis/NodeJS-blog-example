    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.1",
